{
  "manifest_version": 3,
  "name": "Website Blocker",
  "version": "0.2.0",
  "description": "A simple website blocker.",
  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "options_page": "options.html"
}
